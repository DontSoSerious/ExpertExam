<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.expertexam">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".StartUpActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".LoginActivity"
            android:label="Login"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".RegisterActivity"
            android:label="Register"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".Dashboard"
            android:label="Exam List"
            android:theme="@style/AppTheme.NoActionBar"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>

        <activity android:name=".StartExamActivity"
            android:label="Start Exam"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>

        <activity android:name=".ViewResultActivity"
            android:label="Your Result"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>

        <activity android:name=".ViewResultsActivity"
            android:label="Your Results History"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>

        <activity android:name=".EditProfileActivity"
            android:label="Edit Profile"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>

        <activity android:name=".ChangePasswordActivity"
            android:label="Change Password"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>

        <activity android:name=".AddExamActivity"
            android:label="Adding Exam"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
    </application>

</manifest>